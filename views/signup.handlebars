<div class="background responsive-img">
    <div class="container">
        {{!-- <div class="section white"> --}}
        <div class="row">
            <h2 class="header">Meal Plan Registration</h2>
            <p class="grey-text text-darken-3 lighten-3">Welcome to our Meal Plan App where you can budget your
                meals.
            </p>
            <hr class="col 14 m4 s12">
            <br>
            <p class="grey-text text-darken-3 lighten-3">Register an account and start planning right away!</p>
        </div>
        <div class="row">
            <form id="signup-form" class="col 14 m4 s12">
                <div class="row">
                    <div class="input-field col s6">
                        <input id="signup-firstName" type="text" class="validate">
                        <label class = "black-text" for="signup-firstName">First Name</label>
                    </div>
                    <div class="input-field col s6">
                        <input id="signup-lastName" type="text" class="validate">
                        <label class = "black-text" for="signup-lastName">Last Name</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s8">
                        <input value="" id="signup-addressStreet" type="text" class="validate">
                        <label class = "black-text" for="signup-addressStreet">Address</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s6">
                        <input value="" id="signup-addressCity" type="text" class="validate">
                        <label class = "black-text" for="signup-addressCity">City</label>
                    </div>
                    <div class="input-field col s5">
                        <input maxlength="2" id="signup-addressState" type="text" class="validate">
                        <label class = "black-text" for="signup-addressState">State</label>
                    </div>
                    <div class="input-field col s5">
                        <input value="" id="signup-addressZip" type="number" class="validate">
                        <label class = "black-text" for="signup-addressZip">Zip Code</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s8">
                        <input id="signup-password" type="password" class="validate">
                        <label class = "black-text" for="signup-password">Password</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s8">
                        <input id="signup-email" type="email" class="validate">
                        <label class = "black-text" for="signup-email">Email</label>
                    </div>
                </div>
                <button class="btn waves-effect waves-light" type="submit" name="action">Register
                    <i class="material-icons right">send</i>
                </button>
            </form>
        </div>

    </div>
</div>
